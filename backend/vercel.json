{
  "version": 2,
  "buildCommand": "npm run build || echo 'No build step'",
  "env": {
    "VERCEL": "true"
  },
  "functions": {
    "src/server.js": {
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/src/server.js"
    }
  ]
}
